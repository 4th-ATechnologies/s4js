{
	"name": "s4js",
	"version": "1.0.0",
	"description": "A javascript wrapper for the S4 encryption library, using WebAssembly.",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"scripts": {
		"prebuild": "mkdir -p dist && cp wasm/libS4* dist && mkdir -p dist.browser && cp wasm/libS4* dist.browser",
		"build": "tsc && tsc -p tsconfig.browser.json",
		"test": "mocha -r ts-node/register test/*.spec.ts"
	},
	"files": [
		"dist",
		"dist.browser"
	],
	"keywords": [
		"encryption",
		"wasm",
		"WebAssembly",
		"skein",
		"threefish"
	],
	"author": "Robbie Hanson",
	"license": "ISC",
	"devDependencies": {
		"@types/chai": "^4.1.7",
		"@types/lodash": "^4.14.117",
		"@types/mocha": "^5.2.5",
		"@types/node": "^10.12.1",
		"chai": "^4.2.0",
		"lodash": "^4.17.11",
		"mocha": "^5.2.0",
		"ts-node": "^7.0.1",
		"typescript": "^3.1.4"
	}
}
